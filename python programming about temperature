temp=float(input("enter the temperatutre"))
if temp<-273.15:
    print("temp is invalid")
elif temp==-273.15:
    print("temp is absolute 0")
elif temp>=-273.5 and temp==0:
    print("temp is below freezing")
elif temp==0:
    print("temp is freezing point")
elif temp>=0 and temp<=100:
    print("temp is in normal range")
elif temp == 100:
    print("temp is boiling point")
elif temp > 100:
    print("temp is above boiling point")

