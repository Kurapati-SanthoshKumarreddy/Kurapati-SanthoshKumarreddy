days=int(input("number of days"))
charge=int(input("charge per day"))
if days<=5:
    charge=days*2
    print("the net charge",charge)
elif days>=6 and  days<=10:
    charge=days*3
    print("the net charge",charge)
elif days>=11 and days<=15:
    charge=days*4
    print("the net charge",charge)
elif days>15:
        charge=days*5
        print("the net charge",charge)

